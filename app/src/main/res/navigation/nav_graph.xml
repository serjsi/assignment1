<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/signFragmentGraph">
    <fragment
        android:id="@+id/signFragmentGraph"
        android:name="com.shpp.ssierykh.assignment1.ui.sign.SignFragment"
        android:label="fragment_sign"
        tools:layout="@layout/fragment_sign" >
        <action
            android:id="@+id/action_signFragmentGraph_to_myProfileFragmentGraph"
            app:destination="@id/myProfileFragmentGraph"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="false"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/myProfileFragmentGraph"
        android:name="com.shpp.ssierykh.assignment1.ui.my_profile.MyProfileFragment"
        android:label="fragment_my_profile_"
        tools:layout="@layout/fragment_my_profile_" >
        <action
            android:id="@+id/action_myProfileFragmentGraph_to_myContactsFragmentGraph"
            app:destination="@id/myContactsFragmentGraph" />
        <action
            android:id="@+id/action_myProfileFragmentGraph_to_addOrEditContactsDialogFragmentGraph"
            app:destination="@id/addOrEditContactsDialogFragmentGraph" />
    </fragment>
    <fragment
        android:id="@+id/addOrEditContactsDialogFragmentGraph"
        android:name="com.shpp.ssierykh.assignment1.ui.edit_profile.EditProfileContactDialogFragment"
        android:label="dialog_add_or_edit_contact_profile"
        tools:layout="@layout/edit_contact_profile" >
        <argument
            android:name="contactArg"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/myContactsFragmentGraph"
        android:name="com.shpp.ssierykh.assignment1.ui.view_my_contacts.MyContactsFragment"
        android:label="fragment_my_contacts"
        tools:layout="@layout/fragment_my_contacts" >
        <action
            android:id="@+id/action_myContactsFragmentGraph_to_addOrEditContactsDialogFragmentGraph3"
            app:destination="@id/addOrEditContactsDialogFragmentGraph" />
        <action
            android:id="@+id/action_myContactsFragmentGraph_to_contactProfileFragment"
            app:destination="@id/contactProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/contactProfileFragment"
        android:name="com.shpp.ssierykh.assignment1.ui.contact_profile.ContactProfileFragment"
        android:label="fragment_contact_profile_"
        tools:layout="@layout/fragment_contact_profile_" >
        <argument
            android:name="contactArg"
            app:argType="string"
            android:defaultValue='" "' />
    </fragment>
</navigation>